{
    "name": "gitalk",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Git Talk",
        "default_popup": "popup.html"
    },
    "background": {
    	"scripts": ["github.js"],
    	"persistent": false
	},
	"content_scripts": [
    	{
        	"matches": ["http://github.com/*", "https://github.com/*"],
        	"js": ["underscore-min.js", "base64.js","github.js", "content.js", "angular.min.js"],
            "css": ["styling.css"]
    	}
	]
}